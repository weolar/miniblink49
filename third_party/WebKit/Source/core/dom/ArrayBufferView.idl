[
    ImplementedAs=DOMArrayBufferView,
    NoInterfaceObject,
] interface ArrayBufferView {
    readonly attribute ArrayBuffer buffer;
    readonly attribute unsigned long byteOffset;
    readonly attribute unsigned long byteLength;
};
