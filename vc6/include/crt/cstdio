// cstdio standard header
#ifndef _CSTDIO_
#define _CSTDIO_
#ifdef _STD_USING
 #undef _STD_USING
 #include <stdio.h>
 #define _STD_USING
#else
 #include <stdio.h>
#endif /* _STD_USING */
#endif /* _CSTDIO_ */

