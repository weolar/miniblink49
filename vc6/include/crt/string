// template<>
// void std::basic_string<char, std::char_traits<char>, std::allocator<char> >::clear(void);

#pragma warning(default:4231) /* restore previous warning */
#endif		// _DLL

_STD_END
#ifdef  _MSC_VER
#pragma pack(pop)
#endif  /* _MSC_VER */

#endif /* _STRING */

