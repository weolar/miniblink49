// cctype standard header
#ifndef _CCTYPE_
#define _CCTYPE_
#ifdef _STD_USING
 #undef _STD_USING
 #include <ctype.h>
 #define _STD_USING
#else
 #include <ctype.h>
#endif /* _STD_USING */
#endif /* _CCTYPE_ */

