#ifndef _CWCHAR_
#define _CWCHAR_
#ifdef _STD_USING
 #undef _STD_USING
 #include <wchar.h>
 #define _STD_USING
#else
 #include <wchar.h>
#endif /* _STD_USING */
#endif /* _CWCHAR_ */

